                "/dev/konvajs/gif2": "${workspaceFolder}/routes/views/10.dev/02.Client/08.konvajs/02 - gif2",
                "/dev/konvajs/gif3": "${workspaceFolder}/routes/views/10.dev/02.Client/08.konvajs/03 - gif3"
            }
        }
    ]
}