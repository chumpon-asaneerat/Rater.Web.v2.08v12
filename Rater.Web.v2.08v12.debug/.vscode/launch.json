{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            /* 
            required to set chrome debugger first by open chrome shotcut -> properties
            then in target append --remote-debugging-port=9222 then save.

            First start server (node.js) and then run attach to connect to opened browser
            after refresh browser you should see debug console display log from target url.
            */
            "name": "attach admin",
            "type": "chrome",
            "request": "attach",
            "port": 9222,
            "url": "http://localhost:3000",
            "pathMapping": {
                "/": "${workspaceFolder}/routes/views/01.public/home",
                /* edl -> home (admin/exlusive/staff) */
                "/edl/admin": "${workspaceFolder}/routes/views/02.edl/00.home/01.admin",
                "/edl/supervisor": "${workspaceFolder}/routes/views/02.edl/00.home/02.supervisor",
                "/edl/staff": "${workspaceFolder}/routes/views/02.edl/00.home/02.staff",
                /* edl routes */
                "/edl/customer": "${workspaceFolder}/routes/views/03.customer/01.customers",
                "/edl/user": "${workspaceFolder}/routes/views/03.customer/02.users",
                /* customer -> home (admin/exlusive/staff) */
                "/customer/admin": "${workspaceFolder}/routes/views/03.customer/00.home/01.admin",
                "/customer/exclusive": "${workspaceFolder}/routes/views/03.customer/00.home/02.exclusive",
                "/customer/staff": "${workspaceFolder}/routes/views/03.customer/00.home/03.staff",
                /* customer routes */
                "/customer/member": "${workspaceFolder}/routes/views/03.customer/01.members",
                "/customer/device": "${workspaceFolder}/routes/views/03.customer/02.devices",
                "/customer/branch": "${workspaceFolder}/routes/views/03.customer/03.branchs",
                "/customer/org": "${workspaceFolder}/routes/views/03.customer/04.orgs",
                "/customer/report": "${workspaceFolder}/routes/views/03.customer/05.report/01.home",
                "/customer/report/rawvote": "${workspaceFolder}/routes/views/03.customer/05.report/02.rawvote",
                "/customer/report/votesummary": "${workspaceFolder}/routes/views/03.customer/05.report/03.votesummary",
                "/customer/report/barchart": "${workspaceFolder}/routes/views/03.customer/05.report/04.barchart",
                "/customer/report/piechart": "${workspaceFolder}/routes/views/03.customer/05.report/05.piechart",
                "/customer/report/staffcompare": "${workspaceFolder}/routes/views/03.customer/05.report/06.staffcompare",
                "/customer/report/staffperf": "${workspaceFolder}/routes/views/03.customer/05.report/07.staffperf",
                /* device -> home */
                "/rater": "${workspaceFolder}/routes/views/04.device/00.home",
                /* device routes */
                "/rater/register": "${workspaceFolder}/routes/views/04.device/01.register",
                "/rater/setup": "${workspaceFolder}/routes/views/04.device/02.setup",
                "/rater/signin": "${workspaceFolder}/routes/views/04.device/03.signin",
                "/rater/question": "${workspaceFolder}/routes/views/04.device/04.question",
                /* dev routes */
                /* dev routes -> css */
                "/dev/css/animation/sidebar": "${workspaceFolder}/routes/views/10.dev/02.Client/01.css/01.animation.margin",
                /* dev routes -> fabric.js */
                "/dev/fabricjs/basic": "${workspaceFolder}/routes/views/10.dev/02.Client/02.fabricjs/01 - basic",
                "/dev/fabricjs/dualcanvas": "${workspaceFolder}/routes/views/10.dev/02.Client/02.fabricjs/02 - dual canvas",
                "/dev/fabricjs/dualcanvas2": "${workspaceFolder}/routes/views/10.dev/02.Client/02.fabricjs/03 - dual canvas2",
                "/dev/fabricjs/selection": "${workspaceFolder}/routes/views/10.dev/02.Client/02.fabricjs/04 - Selection",
                "/dev/fabricjs/es6": "${workspaceFolder}/routes/views/10.dev/02.Client/02.fabricjs/05 - ES6",
                /* dev routes -> riot.js */
                "/dev/riot/components/webapp": "${workspaceFolder}/routes/views/10.dev/02.Client/03.riotjs/01.webapp",
                "/dev/riot/components/deviceapp": "${workspaceFolder}/routes/views/10.dev/02.Client/03.riotjs/02.deviceapp",
                "/dev/riot/components/card": "${workspaceFolder}/routes/views/10.dev/02.Client/03.riotjs/03.card.layout",
                "/dev/riot/components/card2": "${workspaceFolder}/routes/views/10.dev/02.Client/03.riotjs/04.card.layout2",
                "/dev/riot/components/tool-windows": "${workspaceFolder}/routes/views/10.dev/02.Client/03.riotjs/05.tool-windows",
                "/dev/riot/components/collapse-panels": "${workspaceFolder}/routes/views/10.dev/02.Client/03.riotjs/06.collapse-panels",
                "/dev/riot/components/dual-layout": "${workspaceFolder}/routes/views/10.dev/02.Client/03.riotjs/07.dual.layout",
                "/dev/riot/components/sidebar": "${workspaceFolder}/routes/views/10.dev/02.Client/03.riotjs/08.Sidebar",
                /* dev routes -> layouts */
                "/dev/layouts/dashboard": "${workspaceFolder}/routes/views/10.dev/02.Client/06.layouts/dashboard",
                "/dev/layouts/dashboard2": "${workspaceFolder}/routes/views/10.dev/02.Client/06.layouts/dashboard2",
                /* dev routes -> konva.js */
